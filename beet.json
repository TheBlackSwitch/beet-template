{
    "data_pack": {
        "load": ["src/datapack"],
        "name": "Template Datapack"
    },
    "resource_pack": {
        "load": ["src/resourcepack"],
        "name": "Template Resourcepack"
    },
    "pipeline": [
        "plugins.aliases",
        "mecha"
    ],
    "id": "tp",
    "name": "template",
    "description": "",
    "version": "1.0.0a",
    "require": [
        "bolt"
    ],
    "output": "build",
    "meta": {
        "bolt": {
            "entrypoint": ["tp:main"]
        },
        "aliases": [
            {
                "regex": "\\$version\\$",
                "replace": "v2.0",
                "comment": "The switch lib major version"
            },
            {
                "regex": "\\$patch\\$",
                "replace": "patch-0",
                "comment": "The switch lib major version"
            }
        ]
    }
}